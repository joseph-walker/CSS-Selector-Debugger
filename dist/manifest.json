{
    "manifest_version": 2,
    "name": "CSS Selector Debugger",
    "description": "Debugger for locating DOM elements on a page using CSS selectors",
    "version": "1.0",
    "browser_action": {
        "default_icon": "highlighter.png",
        "default_popup": "index.html"
    },
    "background": {
        "scripts": ["./build/background.js"]
    },
    "permissions": [
        "http://*/*",
        "https://*/*",
        "activeTab",
        "storage",
        "webNavigation"
    ]
}