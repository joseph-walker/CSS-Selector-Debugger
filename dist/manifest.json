{
    "manifest_version": 2,
    "name": "PCA Calltrack Selector Debugger",
    "description": "Debugger for finding phone numbers on a page",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "background": {
        "scripts": ["./build/background.js"]
    },
    "permissions": [
        "http://*/*",
        "https://*/*",
        "activeTab",
        "storage",
        "webNavigation"
    ]
}